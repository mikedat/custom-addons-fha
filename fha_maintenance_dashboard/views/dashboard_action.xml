<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- accion de menu para el dashboard -->
        <record id="maintenance_dashboard_action" model="ir.actions.act_window">
            <field name="name">Dashboard de Mantenimiento</field>
            <field name="res_model">maintenance.request</field>
            <field name="view_mode">kanban</field>
            <field name="view_id" ref="fha_maintenance_dashboard.maintenance_dashboard_custom"/>
